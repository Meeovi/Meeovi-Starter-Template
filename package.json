{
   "name": "starter-template",
   "version": "0.0.4",
   "description": "This is a template for my projects.",
   "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
   "homepage": "https://www.sebastianhilton.com",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Meeovi/Meeovi-Starter-Template"
   },
   "bugs": {
      "url": "https://github.com/Meeovi/Meeovi-Starter-Template/issues"
   },
   "license": "MIT",
   "private": true,
   "scripts": {
      "starter-commit": "git add . && git commit -m \"ready for update\"",
      "starter-update": "git pull \"https://github.com/Meeovi/Meeovi-Starter-Template\"",
      "build": "nuxt build",
      "dev": "nuxt dev",
      "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "postinstall": "nuxt prepare",
      "gqlg": "npx gqlg --schemaFilePath graphql/schema.graphql --destDirPath graphql/framework --depthLimit 2"
   },
   "devDependencies": {
      "@nuxt/image": "^1.3.0",
      "@nuxt/types": "latest",
      "@nuxtjs/turnstile": "^0.7.1",
      "@rollup/plugin-graphql": "^2.0.5",
      "@storefront-ui/nuxt": "^2.5.2",
      "@types/graphql": "^14.5.0",
      "@types/node": "^20.11.13",
      "graphql": "^15.8.0",
      "nuxt": "^3.17.1",
      "typescript": "^5.4.3"
   },
   "dependencies": {
      "@apollo/client": "^3.10.4",
      "@directus/sdk": "^17.0.2",
      "@directus/types": "^12.1.0",
      "@fortawesome/fontawesome-svg-core": "^6.2.1",
      "@fortawesome/free-solid-svg-icons": "^6.2.1",
      "@fortawesome/vue-fontawesome": "^3.0.2",
      "@logto/nuxt": "^1.2.4",
      "@meilisearch/instant-meilisearch": "^0.25.0",
      "@nuxtjs/tailwindcss": "^6.12.0",
      "@stripe/stripe-js": "^3.1.0",
      "@vue/apollo-composable": "^4.2.1",
      "@vueform/nuxt": "^1.13.0",
      "@vueuse/core": "^12.7.0",
      "@vueuse/shared": "^12.5.0",
      "dotenv": "latest",
      "graphql-tag": "^2.12.6",
      "meilisearch": "^0.49.0",
      "nuxt-i18n": "latest",
      "pinia": "^2.3.1",
      "stripe": "^14.23.0",
      "videojs": "^1.0.0",
      "vite-plugin-vuetify": "^2.0.3",
      "vue-instantsearch": "^4.20.6",
      "vue-tsc": "^2.2.10",
      "vuetify": "^3.6.7"
   },
   "workspaces": [
      "layers/*"
   ]
}
